style(type='text/css').
  @font-face {
    font-family: "tele2_displayserif-regular";
    src: local("tele2_displayserif-regular"),
    url("./tele2_displayserif-regular.eot?#iefix") format('embedded-opentype'),
    url("./tele2_displayserif-regular.woff") format('woff'),
    url("./tele2_displayserif-regular.ttf") format('truetype'),
    url("./tele2_displayserif-regular.svg#svgFontName") format('svg');
    font-weight: normal;
    font-style: normal;
  }
  @font-face {
    font-family: "tele2_displayserif-bold";
    src: local("tele2_displayserif-bold"),
    url("./tele2_displayserif-bold.woff") format('woff'),
    url("./tele2_displayserif-bold.ttf") format('ttf'),
    url("./tele2_displayserif-bold.svg#svgFontName") format('svg'),
    url("./tele2_displayserif-bold.eot?#iefix") format('eot');
    font-weight: bold;
    font-style: normal;
  }
  @font-face {
    font-family: "tele2_textsans-regular", sans-serif;
    src: url("./tele2_textsans-regular.eot");
    src: url("./tele2_textsans-regular.eot?#iefix") format('embedded-opentype'),
    url("./tele2_textsans-regular.woff") format('woff'),
    url("./tele2_textsans-regular.ttf") format('truetype'),
    url("./tele2_textsans-regular.svg#svgFontName") format('svg');
    font-weight: normal;
    font-style: normal;
  }
  @font-face {
    font-family: "tele2_textsans-bold", sans-serif;
    src: url("./tele2_textsans-bold.eot");
    src: url("./tele2_textsans-bold.eot?#iefix") format('embedded-opentype'),
    url("./tele2_textsans-bold.woff") format('woff'),
    url("./tele2_textsans-bold.ttf") format('truetype'),
    url("./tele2_textsans-bold.svg#svgFontName") format('svg');
    font-weight: bold;
    font-style: normal;
  }
  .filter-container {
    position: absolute;
    top: calc(100% + 0rem);
    right: 0;
    bottom: auto;
    left: 0;
    border-top: 0.2rem solid #fff;
    opacity: .0;
    z-index: -1;
    transform-origin: 50% 0%;
    transform: translateY(-6rem) scaleY(0);
    transition: all ease-in-out .6s;
  }
